{
    "manifest_version": 3,
    "name": "Trackpad Stream Test",
    "version": "1.0",
    "permissions": ["nativeMessaging", "tabs", "scripting"],
    "host_permissions": ["https://*/*", "http://*/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Start Stream"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
